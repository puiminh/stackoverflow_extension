{
    "manifest_version": 3,
    "name": "StackOverFlow Extensions",
    "description": "Stackoverflow Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://stackoverflow.com/*"
          ]
        }
    ]
  }